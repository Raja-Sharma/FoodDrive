<h2><%= @user.username %>'s profile</h2>

<h3>Donations provided</h3>
<ul>
  <% @user.donations.each do |item| %>
    <li><%= item.name %>, <%= item.category %></li>
  <% end %>
</ul>

<h3>Food claimed</h3>
  <ul>
    <% @user.claimed_foods.each do |item| %>
      <li><%= item.food.name %>, <%= item.food.category %></li>
    <% end %>
  </ul>
